<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.firstscene.FirstSceneLoader">
    <children>
        <AnchorPane fx:id="anchorPane" prefHeight="0.0" prefWidth="200.0">
            <children>
                <Button fx:id="backBTN" alignment="TOP_LEFT" layoutY="-1.0" mnemonicParsing="false" onAction="#exit" style="-fx-background-color: #7a7a7a; -fx-border-radius: 50; -fx-background-radius: 50;" text="Exit" textFill="WHITE">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
        <ImageView fx:id="imageView" fitHeight="134.0" fitWidth="128.0" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
            <image>
                <Image url="@../../../../../580b57fcd9996e24bc43c53e.png" />
            </image>
        </ImageView>
        <VBox fx:id="vBox" alignment="CENTER" spacing="10.0">
            <children>
                <Button fx:id="loginBTN" mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="200.0" style="-fx-background-color: #7a7a7a; -fx-background-radius: 50;" text="Login" textFill="WHITE">
               <font>
                  <Font size="14.0" />
               </font></Button>
                <Button fx:id="signUpBTN" mnemonicParsing="false" onAction="#signUp" prefHeight="26.0" prefWidth="200.0" style="-fx-background-color: #7a7a7a; -fx-background-radius: 50;" text="Sign Up" textFill="#fcfcfc">
               <font>
                  <Font size="14.0" />
               </font></Button>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
